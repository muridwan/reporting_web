                        else
                            $('#jumProdThisYearLast').html(number_format(this['Kontribusi'],2,'.',',')+'<br>(' + number_format($senGSKontrib,2,'.',',') +'% GS)');                        
                        $('#jumKlaimThisYearLast').html(number_format(this['Klaim'],2,'.',',')+'<br>('+number_format($senRKAPKlaim,2,'.',',')+'% RKAP)');
                    });
                },
