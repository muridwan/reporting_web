                beforeSend: function(){
                                $('#spinJumLabaSetPajakYear').show();
                            },
                complete: function(){
                                $('#spinJumLabaSetPajakYear').hide();
                          },
                dataType: 'json',
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                success: function (data) {
                    $.each(data.data, function() {
                        $senLabaSetelahPajak = (this['LabaSetelahPajak']/this['RKAPLabaSetelahPajak']) * 100;
                        $('#jumLabaSetPajakYear').html(number_format(this['LabaSetelahPajak'],2,'.',',')+ '<br>(' + number_format($senLabaSetelahPajak,2,'.',',') + '% RKAP)');
                    });
                },
            });
            $.ajax({
                url:'/FinAcc/GetDataSurplusTabarruPeriode',
                data: {'sdate':firstDayYear,'edate':lastDay},
                beforeSend: function(){
                                $('#spinJumSurplusTabarruYear').show();
                            },
                complete: function(){
                                $('#spinJumSurplusTabarruYear').hide();
                          },
                dataType: 'json',
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                success: function(data){
                    $.each(data.data, function() {
                        $('#jumSurplusTabarruYear').html(number_format(this['SaldoAkhirDanaTabarru'],2,'.',','));
                    });
                },
            });
            //end current
            //Last
            $.ajax({
                url:'/home/GetSumProduksiKlaim/',
                data: {'SDate':fdLast,'EDate':ldLast,'spName':"spSumProduksiKlaim",'stoken':token,'iroleid':roleid},
                beforeSend: function(){
