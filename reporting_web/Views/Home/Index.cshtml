                                $('#spinJumKlaimLast').show();
                          },
                dataType: 'json',
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                success: function (data) {
                    $.each(data.data, function() {
                        $('#jumProdLast').html(number_format(this['Kontribusi'],2,'.',','));
                        $('#jumKlaimLast').html(number_format(this['Klaim'],2,'.',','));
                    });
                },
            });
            $.ajax({
                url:'/home/GetSumProduksiKlaim/',
                data: {'SDate':firstDayYearLast,'EDate':ldLast,'spName':"spSumProduksiKlaim",'stoken':token,'iroleid':roleid},
                beforeSend: function(){
                                $('#spinJumProdThisYearLast').show();
                                $('#spinJumKlaimThisYearLast').show();
                            },
                complete: function(){
                                $('#spinJumProdThisYearLast').hide();
                                $('#spinJumKlaimThisYearLast').hide();
                          },
                dataType: 'json',
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                success: function (data) {
                    $.each(data.data, function() {
                        $senRKAPKontrib = (this['Kontribusi']/this['RKAPKontribusi']) * 100;
                        $senRKAPKlaim = (this['Klaim']/this['RKAPKlaimBruto']) * 100;
