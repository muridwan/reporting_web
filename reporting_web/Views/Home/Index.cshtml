    <script src="~/Scripts/dataTables.responsive.min.js"></script>
    <script src="~/Scripts/AASReport/customaas.js"></script>

    <script>
        $(document).ready(function(){
            var date = new Date();
            var fd = new Date(date.getFullYear(), date.getMonth(), 1);
            var ld = new Date(date.getFullYear(), date.getMonth() + 1, 0);
            var firstDay = getFormattedDate(fd);
            var lastDay = getFormattedDate(ld);
            var monthYear = getFormattedMonth(date);
            var firstDayYear = getFirstDayYear(date);
            var thisYear = date.getFullYear();
            var fdLast = getFirstDateLast(fd);
            var viewFirstDayYear = formatDate(firstDayYear);
            var viewFirstDay = formatDate(firstDay);
            var viewLastDay = formatDate(lastDay);
            var viewFdLast =formatDate(fdLast);
            var viewFirstDayYear = formatDate(firstDayYear);

            date = new Date(fdLast);
            var ldLast = new Date(date.getFullYear(), date.getMonth() + 1, 0);
            ldLast = getFormattedDate(ldLast);
            var firstDayYearLast = getFirstDayYear(date);
            var monthYearLast = getFormattedMonth(date);
            var viewFirstDayYearLast = formatDate(firstDayYearLast);
            var viewLdLast = formatDate(ldLast);
            var token = "@Session["Token"]";
            var roleid = @Session["RoleId"];
            //current
            $.ajax({
                url:'/home/GetSumProduksiKlaim/',
                data: {'SDate':firstDay,'EDate':lastDay,'spName':"spSumProduksiKlaim",'stoken':token,'iroleid':roleid},
                beforeSend: function(){
                                $('#spinJumProd').show();
                                $('#spinJumKlaim').show();
                            },
                complete: function(){
                                $('#spinJumProd').hide();
                                $('#spinJumKlaim').hide();
                          },
                dataType: 'json',
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                success: function (data) {
                    $.each(data.data, function() {
                        $('#jumProd').html(number_format(this['Kontribusi'],2,'.',','));
                        $('#jumKlaim').html(number_format(this['Klaim'],2,'.',','));
                    });
                },
            });
            $.ajax({
                url:'/home/GetSumProduksiKlaim/',
                data: {'SDate':firstDayYear,'EDate':lastDay,'spName':"spSumProduksiKlaim",'stoken':token,'iroleid':roleid},
                beforeSend: function(){
                                $('#spinJumProdThisYear').show();
                                $('#spinJumKlaimThisYear').show();
                            },
                complete: function(){
                                $('#spinJumProdThisYear').hide();
                                $('#spinJumKlaimThisYear').hide();
                          },
                dataType: 'json',
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                success: function (data) {
                    $.each(data.data, function() {
                        $senRKAPKontrib = (this['Kontribusi']/this['RKAPKontribusi']) * 100;
                        $senRKAPKlaim = (this['Klaim']/this['RKAPKlaimBruto']) * 100;
                        $senGSKontrib = (this['Kontribusi']/this['GSKontribusi'])*100;
                        if(roleid<10)
                            $('#jumProdThisYear').html(number_format(this['Kontribusi'],2,'.',',')+'<br>(' + number_format($senRKAPKontrib,2,'.',',') +'% RKAP)');
                        else
                            $('#jumProdThisYear').html(number_format(this['Kontribusi'],2,'.',',')+'<br>(' + number_format($senGSKontrib,2,'.',',') +'% GS)');
                        $('#jumKlaimThisYear').html(number_format(this['Klaim'],2,'.',',')+'<br>('+number_format($senRKAPKlaim,2,'.',',') + '% RKAP)');
                    });
                },
            });
            $.ajax({
                url:'/FinAcc/GetDataLabaRugiPeriode/',
                data: {'sdate':firstDay,'edate':lastDay,'branch':"AS",'stoken':token,'iroleid':roleid},
                beforeSend: function(){
                                $('#spinJumLabaSetPajak').show();
                            },
                complete: function(){
                                $('#spinJumLabaSetPajak').hide();
                          },
                dataType: 'json',
                type: 'GET',
                contentType: 'application/json; charset=utf-8',
                success: function (data) {
                    $.each(data.data, function() {
                        $('#jumLabaSetPajak').html(number_format(this['LabaSetelahPajak'],2,'.',','));
                    });
                },
            });
            $.ajax({
                url:'/FinAcc/GetDataLabaRugiPeriode/',
