    <script>
            $(document).ready(function () {
                $('#btnsavechanges').click(function (){
                    if ($("#newpass").val()!=$("#repass").val())
                        alert ("Retype Password not same with New Password");
                    else
                    {
                        var data = {
                            //"userid": @Session["Empid"],
                            "password": $("#oldpass").val(),
                            "newpassword": $("#newpass").val()
                        };

                        $.ajax({
                            url: "/Login/ChangePassword",
                            type: "POST",
                            data: JSON.stringify(data),
                            dataType: "json",
                            contentType: "application/json",
                            success: function (response) {
                                if (response.Success) {
                                    @*$.get("@Url.Action("Home", "Index")", function (data) {
                                        $('.container').html(data);
                                    });*@
                                    alert("Success!");
                                    $("#modal-changpass").modal('hide');
                                 }
                                else
                                {
                                    alert("Failed.");
                                }
                            },
                            error: function () {
                                console.log('Change Password Failed!');
                            }
                        });
                    }
                });

                $('#signout').click(function () {
                    //var data = {
                    //    "userid": $("#userid").val(),
                    //    "password": $("#password").val()
                    //};
                    $.ajax({
                        url: "/Login/signout",
                        type: "POST",
                        //data: JSON.stringify(data),
                        dataType: "json",
                        contentType: "application/json",
                        success: function (response) {
                            if (response.Success) {
                                @*$.get("@Url.Action("Home", "Index")", function (data) {
                                    $('.container').html(data);
                                });*@
                                alert("User Sign Out!");
                                window.location.href = "@Url.Action("Login", "Login")";
                             }
                            else
                            {
                                alert("Error.");
                                window.location.href = "@Url.Action("Login", "Login")";
                            }
                        },
                        error: function () {
                            console.log('Login Fail!!!');
                        }
                    });
                });
            });
    </script> 

    @RenderSection("scripts", required: false)
</body>
</html>

