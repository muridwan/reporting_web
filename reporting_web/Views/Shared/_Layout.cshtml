                        @foreach (var item in ViewBag.MenuParent)
                        {
                            <li class="nav-item">
                                <a href="#" class="nav-link">
                                    <i class="nav-icon fas fa-table"></i>
                                    <p>
                                        @item.Title
                                        <i class="fas fa-angle-left right"></i>
                                    </p>
                                </a>                                
                                <ul class="nav nav-treeview">
                                    @foreach(var item2 in ViewBag.SubMenu1)
                                    {
                                        if(@item2.Parent==@item.IdMenu)
                                        {                                            
                                            <li class="nav-item">
                                                @if(@item2.UrlMenu=="#")
                                                {                                                    
                                                    <a href="#" class="nav-link">
                                                        <i class="nav-icon fas fa-table"></i>
                                                        <p>
                                                            @item2.Title
                                                            <i class="fas fa-angle-left right"></i>
                                                        </p>
                                                    </a>
                                                    foreach (var item3 in ViewBag.SubMenu2)
                                                    {
                                                        if (@item3.Parent == @item2.IdMenu)
                                                        {
                                                            <ul class="nav nav-treeview">
                                                                <li class="nav-item">
                                                                    <a href="@item3.UrlMenu" class="nav-link">
                                                                        <i class="far fa-circle nav-icon"></i>
                                                                        <p>@item3.Title</p>
                                                                    </a>
                                                                </li>
                                                            </ul>
                                                        }
                                                    }
                                                }
                                                else
                                                {
                                                    <a href="@item2.UrlMenu" class="nav-link">
                                                        <i class="far fa-circle nav-icon"></i>
                                                        <p>@item2.Title</p>
                                                    </a>
                                                }
                                            </li>
                                        }
                                    }
                                </ul>
                            </li>                            
                        }

